{
    "version": 2,
    "builds": [
        {
            "src": "server/index.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "mobile/dist/**",
                    "server/**"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "server/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "server/index.js"
        }
    ]
}