{
    "version": 2,
    "builds": [
        {
            "src": "test-api.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "mobile/dist/**"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "test-api.js"
        },
        {
            "src": "/(.*)",
            "dest": "test-api.js"
        }
    ]
}