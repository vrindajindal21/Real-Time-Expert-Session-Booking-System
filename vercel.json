{
    "version": 2,
    "builds": [
        {
            "src": "test-api.js",
            "use": "@vercel/node"
        },
        {
            "src": "mobile/dist/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "test-api.js"
        },
        {
            "src": "/(.*)",
            "dest": "mobile/dist/$1"
        }
    ]
}